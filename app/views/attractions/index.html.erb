<%=link_to("New Attraction",new_attraction_path) if @user.admin %>
<%=@attractions.each do |attraction| %>
  <%=attraction.name %>
  <%=link_to("Go on #{attraction.name}",attraction_path(attraction)) if !@user.admin%>
  <%= link_to("Show #{attraction.name}",attraction_path(attraction)) if @user.admin %>
<%end%>